{
  "name": "bagoesz21/laravel-image-handler",
  "description": "Laravel image handler",
  "license": "MIT",
  "authors": [
    {
      "name": "BEEP",
      "homepage": "https://github.com/bagoesz21",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/bagoesz21/laravel-image-handler",
  "keywords": [
    "Laravel",
    "LaravelImageHandler",
    "image",
    "php",
    "image handler"
  ],
  "require": {
    "php": "^7.3|^8.0",
    "illuminate/support": "~7|~8|~9"
  },
  "require-dev": {
    "phpunit/phpunit": "~9.0",
    "orchestra/testbench": "~7"
  },
  "autoload": {
    "psr-4": {
      "Bagoesz21\\LaravelImageHandler\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bagoesz21\\LaravelImageHandler\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Bagoesz21\\LaravelImageHandler\\LaravelImageHandlerServiceProvider"
      ],
      "aliases": {
        "LaravelImageHandler": "Bagoesz21\\LaravelImageHandler\\Facades\\LaravelImageHandler"
      }
    }
  }
}
